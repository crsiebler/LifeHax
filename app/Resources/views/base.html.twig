{% stylesheets
    'less/main.less'
    '@MopaBootstrapBundle/Resources/bootstrap/less/responsive.less'
debug=false %}
<link href="{{ asset_url }}" type="text/css" rel="stylesheet" media="screen" />
{% endstylesheets %}
{% endblock head_style %}

